{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "rootDir": "./src/",
    "outDir": "./dist",
  },
  "exclude": ["node_modules"],
  "include": [
    "src"
  ],
}
